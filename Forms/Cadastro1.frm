Public Sub ShowFields()
    For i = 0 To 3
        txtsocio(i).Text = rsSoc.Fields(i)
    Next
    txtsocio(5).Text = rsSoc!Número
    rsCep.Seek rsSoc!CodRua, adSeekFirstEQ
    rsLog.Seek rsCep!codLog, adSeekFirstEQ
    mskCep.Text = rsCep!CEP
    txtsocio(4).Text = rsLog!Logradouro & " " & rsCep!rua
    ComboLetra.Text = rsSoc!letra
    CIDADE = Mid(rsCep!CodBai, 1, 6)
    rsCid.Seek CIDADE, adSeekFirstEQ
    If Not rsCid.EOF And Not rsCid.BOF Then
        txtsocio(7).Text = rsCid!CIDADE
        txtsocio(8).Text = rsCid!Uf
    End If
    rsBai.Seek rsCep!CodBai, adSeekFirstEQ
    txtsocio(6).Text = rsBai!Extenso
End Sub
Private Sub Form_KeyPress(KeyAscii As Integer)
   If KeyAscii = 13 Or KeyAscii = 10 Then
       SendKeys "{TAB}"
       KeyAscii = 0
    End If
    If KeyAscii = 27 Then
        SAIR
    End If
    KeyAscii = Asc(UCase(Chr(KeyAscii)))
End Sub
Private Sub Desabilita()
    For i = 0 To 7
        txtsocio(i).Enabled = False
    Next
    txtsocio(8).Enabled = False
    ComboLetra.Enabled = False
    mskCep.Enabled = False
End Sub
Private Sub Habilita()
    For i = 1 To 3
        txtsocio(i).Enabled = True
    Next
    ComboLetra.Enabled = True
    txtsocio(0).Enabled = False
    txtsocio(2).Enabled = False
End Sub
Private Sub SAIR()
    On Error GoTo forçar_sair
        Close All
        Unload Me
forçar_sair:
        Unload Me
End Sub
Private Sub LimpaCampos()
    For i = 0 To 8
        txtsocio(i).Text = Empty
    Next
    ComboLetra.Text = Empty
 
    mskCep.Mask = ""
    mskCep.Text = ""
    mskCep.Mask = "#####-###"
End Sub
Private Sub mskCep_Validate(Cancel As Boolean)
    rsCep.Seek mskCep.Text
    If Not rsCep.EOF Then
        rsLog.Seek rsCep!codLog, adSeekFirstEQ                  'Busca do logradouro
        txtsocio(4).Text = rsLog!Logradouro & " " & rsCep!rua   'Monta a string do endereço
        CIDADE = Mid(rsCep!CodBai, 1, 6)                        'Pega o código da cidade
        rsCid.Seek CIDADE, adSeekFirstEQ                        'Busca a cidade
        txtsocio(7).Text = rsCid!CIDADE                         'Carrega o nome da cidade
        txtsocio(8).Text = rsCid!Uf                             'Carrega a Sigla do Estado
        rsBai.Seek rsCep!CodBai, adSeekFirstEQ                  'Busca o Bairro do CEP
        txtsocio(6).Text = rsBai!Extenso                        'Pega o extenso do bairro
        txtsocio(5).Enabled = True                              'Posiciona no número do endereço
        Me.txtsocio(5).SetFocus
    Else
        MsgBox "CEP não localizado! Entre com o endereço!", vbCritical + vbOKOnly
        Me.txtsocio(4).Enabled = True
        Me.txtsocio(4).SetFocus
       
    End If
End Sub
Private Sub txtsocio_GotFocus(Index As Integer)
    Me.txtsocio(Index).Enabled = True
    Me.txtsocio(Index).SetFocus
End Sub
Private Sub txtsocio_Validate(Index As Integer, Cancel As Boolean)
    Select Case Index
        Case 4
            SQL = "SELECT DISTINCT RUA FROM RUAS WHERE RUA LIKE '" & _
                    txtsocio(4).Text & "%' ORDER BY RUA"
            rsTMPRua.Open SQL, cnCep
            rsTMPRua.MoveFirst

            While Not rsTMPRua.EOF
                
                cboTMP(0).AddItem rsTMPRua!rua
                rsTMP.MoveNext
            Wend
            ProgressBar1.Visible = False
    End Select
End Sub


