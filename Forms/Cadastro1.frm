    With cnCep
        .ConnectionString = "Provider = Microsoft.Jet.OLEDB.4.0;Data Source=F:\Sistemas\Anhanguera\CEP.mdb;"
        .CursorLocation = adUseClient
        .Open
    End With
    
    With rsSoc
