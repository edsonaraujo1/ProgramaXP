        rsSoc.MoveFirst
        ShowFields
    Case 1
        lblMessage = " "
        rsSoc.MovePrevious
        If rsSoc.BOF Then
            rsSoc.MoveLast
        End If
        ShowFields
    Case 2
        lblMessage = " "
        rsSoc.MoveNext
        If rsSoc.EOF Then
            rsSoc.MoveFirst
        End If
        ShowFields
    Case 3
        lblMessage = "Final"
        rsSoc.MoveLast
        ShowFields
    Case 4
        For i = 0 To 8
            cmdSocios(i).Enabled = False
        Next
        cmdSocios(9).Enabled = True
        cmdSocios(10).Enabled = True
        lblMessage = "Incluir"
        rsSoc.MoveLast
        novocod = rsSoc!Código + 1
        LimpaCampos
        Habilita
        rsSoc.AddNew
        
        rsSoc!Código = novocod
        txtsocio(0).Text = novocod
        
        txtsocio(1).Enabled = True
        txtsocio(1).SetFocus
        
        txtsocio(2).Enabled = False
        mskCep.Enabled = True
        
        txtsocio(2).Text = Format(Date, "dd/mm/yyyy")
        
   Case 8
        SAIR
End Select
End Sub

